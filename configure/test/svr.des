#ifndef SVR_DES
#define SVR_DES
struct ubsvr
{
	string name = default("name");
	int32_t thread_num = default(30);
	int32_t port = default(50000);
	int32_t read_bufsize = default(1024000);
	int32_t write_bufsize = default(1024000);
	int32_t read_timeout = default(500);
	int32_t write_timeout = default(500);
	int32_t connect_timeout = default(200);
	int32_t queue_size = default(100);
	int32_t socket_size = default(512);
};


#endif
