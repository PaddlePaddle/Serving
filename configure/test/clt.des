#ifndef CLT_DES
#define CLT_DES

struct ubclt	//设置ub客户端
{
	string name = default("default");	//请求服务器文件名
	string ip[];	//服务器ip数组 @ip 127.0.0.1
	int32_t port = default(5000);	//请求服务器数组
	int32_t read_timeout = default(500);
	int32_t write_timeout = default(500);
	int32_t connect_timeout = default(200);
};

#endif
